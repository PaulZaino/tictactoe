<%= form_for @game do |f| %>

  <fieldset>
    <%= f.bootstrap_text_field :dimension %>
    <%= f.bootstrap_collection_select :experimental_condition_id, ExperimentalCondition.all, :id, :name, :label => "Condition" %>
  </fieldset>
  <div class="actions">
    <%= f.submit "Start Game", :class => "btn primary"%>
  </div>
<% end %>
